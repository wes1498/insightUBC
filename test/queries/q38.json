{
    "title": "using ** in IS (before and after)",
    "query": {
        "WHERE": {
            "AND": [
                {
                    "IS": {
                        "courses_dept": "*at*"
                    }
                },
                {
                    "EQ": {
                        "courses_avg": 95
                    }
                },
                {
                    "GT": {
                        "courses_pass": 300
                    }
                },
                {
                    "LT": {
                        "courses_fail": 0
                    }
                },
                {
                    "NOT": {
                        "EQ": {
                            "courses_avg": 0
                        }
                    }
                }
            ]
        },
        "OPTIONS": {
            "COLUMNS": [
                "courses_dept",
                "courses_avg"
            ],
            "ORDER": "courses_avg"
        }
    },
    "isQueryValid": true,
    "result": []
}
