{
    "title": "using ** in IS (before and after)",
    "query": {
        "WHERE": {
            "OR": [
                {
                    "AND": [
                        {
                            "NOT": {
                                "IS": {
                                    "courses_dept": "*at*"
                                }
                            }
                        },
                        {
                            "GT": {
                                "courses_avg": 98.9
                            }
                        }
                    ]
                }
            ]
        },
        "OPTIONS": {
            "COLUMNS": [
                "courses_dept",
                "courses_avg"
            ],
            "ORDER": "courses_avg"
        }
    },
    "isQueryValid": true,
