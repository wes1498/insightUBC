{
    "title": "a nested empty wildcard",
    "query": {
        "WHERE": {
            "AND": [
                {
                    "IS": {
                        "courses_dept": "*"
                    }
                },
                {
                    "EQ": {
                        "courses_pass": 300
                    }
                }
            ]
        },
        "OPTIONS": {
            "COLUMNS": [
                "courses_dept",
                "courses_avg"
            ],
            "ORDER": "courses_avg"
        }
    },
    "isQueryValid": true,
    "result": [
        {"courses_dept":"chem","courses_avg":67.56},
        {"courses_dept":"psyc","courses_avg":67.69},
        {"courses_dept":"psyc","courses_avg":69.94},
        {"courses_dept":"eosc","courses_avg":73.23},
        {"courses_dept":"biol","courses_avg":79.22},
        {"courses_dept":"apsc","courses_avg":82.66}
    ]
}
